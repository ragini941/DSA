#include <iostream>

using namespace std;

int main()
{int arr[50],i,n,total;

cout<<"enter the number of elements in array "<<endl;

cin>>n;
total=(n+1)*(n+2)/2;
cout <<"total = "<<total<<endl;
for(i=0;i<=n-1;i++){
    cout<<"elements in an array "<<endl;
    cin>>arr[i];

    total-=arr[i];
    cout<<"missing element "<<total<<endl;
}
    

    return 0;
}

